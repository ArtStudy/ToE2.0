<Page x:Class="GameСreator.Pages.EditLevelPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GameСreator.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
       DataContext="{Binding  Main, Source={StaticResource  Locator}}"
      Title="EditLevelPage">

    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="194,35,0,0" TextWrapping="Wrap" Text="{Binding CurrentLevel.IDLevel}" VerticalAlignment="Top" Width="276"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="225,63,0,0" TextWrapping="Wrap" Text="{Binding CurrentLevel.NameLavel}"  VerticalAlignment="Top" Width="245"/>
        <Label Content="ID" HorizontalAlignment="Left" Margin="31,40,0,0" VerticalAlignment="Top"/>
        <Label Content="Name" HorizontalAlignment="Left" Margin="31,63,0,0" VerticalAlignment="Top"/>
        <TextBlock Text="Level." HorizontalAlignment="Left" Margin="194,64,0,0" VerticalAlignment="Top" Width="29"/>

    </Grid>
</Page>
